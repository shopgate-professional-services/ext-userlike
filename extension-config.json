{
  "id": "@shopgate-project/userlike",
  "version": "1.0.2",
  "components": [
    {
      "id": "Subscribers",
      "path": "frontend/subscriptions.js",
      "type": "subscribers"
    }
  ],
  "configuration": {
    "sdkUrl": {
      "type": "admin",
      "destination": "frontend",
      "default": "https://userlike-cdn-widgets.s3-eu-west-1.amazonaws.com/cd9bc42380a21d231a607bbea4f01c2fa1424580cf9e3a860f25db983eaf24aa.js",
      "params": {
        "type": "text",
        "label": "Url of userlike JavaScript sdk"
      }
    },
    "pagesWithoutWidget": {
      "type": "admin",
      "destination": "frontend",
      "default":[
        "/cart",
        "/login"
      ],
      "params": {
        "type": "json",
        "label": "Page patterns which should not have the chat widget"
      }
    }
  }
}
